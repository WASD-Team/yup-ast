{
  "presets": [
    [
      "env",
      {
        "modules": false
      }
    ],
      "node8",
      "stage-0"
  ],
  "env": {
    "production": {
      "only": ["app"],
      "plugins": [
        "transform-react-constant-elements",
        "transform-react-inline-elements"
      ]
    },
    "test": {
      "plugins": ["transform-es2015-modules-commonjs", "dynamic-import-node"]
    }
  }
}
